    <LinearLayout
        android:id="@+id/linear_layout_end"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        android:orientation="vertical"
        android:visibility="visible"
        android:background="@color/white"
        android:paddingBottom="10dp"
        >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingStart="20dp"
        android:paddingTop="10dp"
        android:paddingEnd="11dp"
        android:paddingBottom="9dp"
        android:background="#F4F4F4"
        >
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Bill total:"
            android:textSize="12sp"
            android:fontFamily="@font/inter_medium"
            android:textColor="#000000"
            android:layout_weight=".15"
            android:layout_gravity="start"
            android:gravity="start"
            android:maxLines="1"
            android:visibility="visible"
            />
        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="AED 250.00000000"
            android:textSize="12sp"
            android:fontFamily="@font/inter_semibold"
            android:textColor="#000000"
            android:layout_weight=".5"
            android:layout_gravity="start"
            android:gravity="start"
            android:maxLines="1"
            />
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="VIEW DETAILED BILL"
                android:textSize="12sp"
                android:fontFamily="@font/inter_medium"
                android:textColor="#CE6A59"
                android:layout_weight=".8"
                android:layout_gravity="end"
                android:gravity="end"
                android:paddingEnd="2dp"
                android:maxLines="1"
                />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingStart="21dp"
        android:paddingTop="18dp"
        android:paddingBottom="11dp"
        android:paddingEnd="12dp"
        android:background="@color/white"
        >
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"
            >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Deliver to"
                    android:textSize="12sp"
                    android:textColor="@color/black"
                    android:fontFamily="@font/inter_medium"
                    android:layout_weight=".25"/>
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Home"
                    android:layout_weight="1"
                    android:textSize="12sp"
                    android:textColor="@color/black"
                    android:fontFamily="@font/inter_semibold"
                    />
            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Sector 12, Deira, Dubai, United Arab Emirat .."
                android:textSize="11sp"
                android:textColor="#888888"
                android:fontFamily="@font/inter_regular"
                android:layout_marginTop="6dp"
                android:paddingEnd="10dp"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight=".3"
            android:layout_gravity="end"
            android:gravity="end">
            <include
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                layout="@layout/layout_delivery_type"
                />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/proceed_button_border"
        android:paddingTop="12dp"
        android:paddingBottom="12dp"
        android:paddingStart="97dp"
        android:paddingEnd="95dp"
        android:layout_marginTop="5dp"
        android:layout_marginStart="21dp"
        android:layout_marginBottom="17dp"
        android:layout_marginEnd="12dp"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Proceed to Pay"
            android:textSize="16sp"
            android:fontFamily="@font/inter_medium"
            android:textColor="@color/white"
            android:paddingHorizontal="15dp"
            />
    </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>